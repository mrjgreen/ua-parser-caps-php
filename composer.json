{
  "name": "mrjgreen/ua-parser-caps-php",
  "type": "library",
  "description": "Load device capabilities from UA-Parser output",
  "keywords": [
    "user agent",
    "ua-parser",
    "ua-capabilities"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Joe Green",
      "email": "joe.green0991@gmail.com"
    }
  ],
  "require": {
    "symfony/yaml": "^2.7",
    "symfony/console": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "UACapabilities\\": "src"
    }
  },
  "bin": [
    "bin/uacaps"
  ],
  "scripts": {
    "post-dumpautoload-cmd": [
      "bin/uacaps update"
    ]
  }
}
